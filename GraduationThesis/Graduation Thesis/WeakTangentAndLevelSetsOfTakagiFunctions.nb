Cell[TextData[{
 "In this paper, we study graphs of the following functions",
 StyleBox[" ",
  FontColor->RGBColor[1., 0., 0.]],
 StyleBox[" ",
  FontWeight->"Bold",
  FontColor->RGBColor[1., 0., 0.]],
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["T", "TI"], 
          RowBox[{
            StyleBox["a", "TI"], ",", 
            StyleBox["b", "TI"]}]], "\[LongEqual]", 
         UnderoverscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["n", "TI"], "\[LongEqual]", "0"}], "\[Infinity]", 
          LimitsPositioning -> True], 
         SuperscriptBox[
          StyleBox["a", "TI"], 
          StyleBox["n", "TI"]], 
         StyleBox["T", "TI"], 
         RowBox[{"(", 
           RowBox[{
             SuperscriptBox[
              StyleBox["b", "TI"], 
              StyleBox["n", "TI"]], 
             StyleBox["x", "TI"]}], ")"}]}], TraditionalForm], "errors" -> {},
      "input" -> "T_{a,b}=\\sum_{n=0}^\\infty a^nT(b^nx)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],
  FontColor->RGBColor[1., 0., 0.],ExpressionUUID->
  "a769d649-b15b-ac4a-bec6-89845376c346"],
 " where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["a", "TI"], ",", 
         StyleBox["b", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "a,b", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3158ba67-7011-b544-936f-5923a0bb3116"],
 " are real parameters ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["a", "TI"], ",", 
         StyleBox["b", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "a,b", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "4811b2a4-ced9-2b4d-901d-db6bab06ec97"],
 " such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[<|"boxes" -> FormBox[
        RowBox[{
          StyleBox["a", "TI"], "<", "1", ",", 
          StyleBox["b", "TI"], ">", "1", ",", 
          StyleBox["a", "TI"], 
          StyleBox["b", "TI"], "\[GreaterEqual]", "1"}], TraditionalForm], 
      "errors" -> {}, "input" -> "a<1,b>1,ab\\ge1", "state" -> "Boxes"|>,
     "TeXAssistantTemplate"], " "}], TraditionalForm]],ExpressionUUID->
  "46eb7bf0-05c3-cb48-b7aa-9a75c8cba223"],
 "and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["T", "TI"], ":", 
         StyleBox["R", "TI"], "\[Rule]", 
         StyleBox["R", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "T:\\mathcal{R}\\rightarrow\\mathcal{R}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "219c91d6-26e8-ae40-8031-5d46aa714c37"],
 " is the ",
 StyleBox["tent map",
  FontWeight->"Bold",
  FontColor->RGBColor[1., 0., 0.]],
 " which has period 1 and defined on the unit interval as follows, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["T", "TI"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "\[LongEqual]", 
         RowBox[{"{", 
           Cell[
            BoxData[
             FormBox[
              GridBox[{{
                 Cell[
                  TextData[{" ", 
                    Cell[
                    BoxData[
                    RowBox[{
                    StyleBox["x", "TI"], "\[NonBreakingSpace]", 
                    "\[NonBreakingSpace]", "\[NonBreakingSpace]", 
                    StyleBox["x", "TI"], "\[Element]", 
                    RowBox[{"[", 
                    RowBox[{"0", ",", 
                    FractionBox["1", "2"]}], "]"}]}]], "InlineFormula"], 
                    " "}]]}, {
                 Cell[
                  TextData[{" ", 
                    Cell[
                    BoxData[
                    RowBox[{"1", "-", 
                    StyleBox["x", "TI"], "\[NonBreakingSpace]", 
                    StyleBox["x", "TI"], "\[Element]", 
                    RowBox[{"[", 
                    RowBox[{
                    FractionBox["1", "2"], ",", "1"}], "]"}]}]], 
                    "InlineFormula"], " "}]]}}], TraditionalForm]]]}]}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "T(x) = \\begin{cases}x~~~x\\in[0,\\frac{1}{2}]\\\\1-x~x\\in[\\frac{1}{2}\
,1]\\end{cases}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],
  FontColor->RGBColor[1., 0., 0.],ExpressionUUID->
  "0da3a6fc-1540-cc4d-8ec9-a0212d154d95"],
 " "
}], "Text",
